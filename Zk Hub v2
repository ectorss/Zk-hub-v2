-- ZK HUB v3.0 - COMPACT
local P=game:GetService("Players")local RS=game:GetService("RunService")local W=game:GetService("Workspace")local C=W.CurrentCamera local L=game:GetService("Lighting")local plr=P.LocalPlayer local pg=plr:WaitForChild("PlayerGui")local DS=pcall(Drawing.new,"Square")
local Cor={B=Color3.fromRGB(10,10,10),S=Color3.fromRGB(26,26,26),P=Color3.fromRGB(0,102,255),T=Color3.fromRGB(255,255,255),D=Color3.fromRGB(180,180,200),G=Color3.fromRGB(0,200,0),R=Color3.fromRGB(255,70,70),I=Color3.fromRGB(60,60,70)}
local CF={AA=true,SM=18,AT="HEAD",FV=150,ESP=false,HB=false,N=true,FPS=false,BO=false}local F,UI,ED,HD,CT=nil,nil,{},{},nil
local hue=0 local function U_F()if not CF.AA then if F then F:Destroy()F=nil end return end if not F then F=Instance.new("ScreenGui")F.Name="F"F.Parent=game:GetService("CoreGui")F.ResetOnSpawn=false F.ZIndexBehavior=Enum.ZIndexBehavior.Sibling F.DisplayOrder=999
local c=Instance.new("Frame")c.Name="C"c.BackgroundTransparency=1c.BorderSizePixel=0c.Parent=F Instance.new("UICorner",c).CornerRadius=UDim.new(1,0)local s=Instance.new("UIStroke",c)s.Thickness=2s.Transparency=0.3 end
local c=F:FindFirstChild("C")if c then hue=(hue+0.005)%1 c.UIStroke.Color=Color3.fromHSV(hue,1,1)local s=CF.FV*2 c.Size=UDim2.new(0,s,0,s)c.Position=UDim2.new(0.5,-CF.FV,0.5,-CF.FV)end end
RS.RenderStepped:Connect(U_F)
local FT=Drawing.new("Text")FT.Visible=false FT.Color=Color3.new(0,1,0)FT.Size=18 FT.Center=false FT.Outline=true FT.Position=Vector2.new(10,10)FT.Text="FPS: 60"local st,fc,fps=nil,0,60
RS.RenderStepped:Connect(function()if CF.FPS then FT.Visible=true fc=fc+1 if not st then st=tick()end local e=tick()-st if e>=0.5 then fps=math.floor(fc/e+0.5)st=tick()fc=0 end FT.Text="FPS: "..fps
if fps>=60 then FT.Color=Color3.new(0,1,0)elseif fps>=30 then FT.Color=Color3.new(1,1,0)else FT.Color=Color3.new(1,0,0)end else FT.Visible=false end end)
local OS={}local function FB(e)if e then if next(OS)==nil then OS={GS=W.CurrentCamera.GlobalShadows,FE=L.FogEnd,DE=W.CurrentCamera.DecalsEnabled,TT=pg:GetTopbarTransparency()}end
W.CurrentCamera.GlobalShadows=false L.FogEnd=9e9 W.CurrentCamera.DecalsEnabled=false pg:SetTopbarTransparency(1)for _,v in ipairs(L:GetChildren())do if v:IsA("BloomEffect")or v:IsA("BlurEffect")or v:IsA("ColorCorrectionEffect")or v:IsA("SunRaysEffect")then v.Enabled=false end end
pcall(function()for _,o in ipairs(W:GetDescendants())do if o:IsA("ParticleEmitter")and(o.Position-C.CFrame.Position).Magnitude>100 then o.Enabled=false end end end)else
if OS.GS~=nil then W.CurrentCamera.GlobalShadows=OS.GS L.FogEnd=OS.FE W.CurrentCamera.DecalsEnabled=OS.DE pg:SetTopbarTransparency(OS.TT)end
for _,v in ipairs(L:GetChildren())do if v:IsA("BloomEffect")or v:IsA("BlurEffect")or v:IsA("ColorCorrectionEffect")or v:IsA("SunRaysEffect")then v.Enabled=true end end end end
local function IEM(m)if not m or m==plr.Character then return false end if not m:FindFirstChild("Humanoid")or not m:FindFirstChild("HumanoidRootPart")then return false end local h=m.Humanoid if h.Health<=0 then return false end
local p=P:GetPlayerFromCharacter(m)if p then if p==plr then return false end if plr.TeamColor~=BrickColor.new("White")and p.TeamColor~=BrickColor.new("White")then return plr.TeamColor~=p.TeamColor end
local mt,tt=plr.Team,p.Team if mt and tt then return mt~=tt end return true end local n=m.Name:lower()if n:find("civil")or n:find("civilian")or n:find("npc")or n:find("civ")then return false end return true end
local function GE()local e={}for _,o in ipairs(W:GetDescendants())do if o:IsA("Model")and IEM(o)then table.insert(e,{M=o,R=o.HumanoidRootPart,H=o.Humanoid})end end return e end
local function GBT()if not CF.AA then return nil end local char=plr.Character if not char then return nil end local r=char:FindFirstChild("HumanoidRootPart")if not r then return nil end
local sc=Vector2.new(C.ViewportSize.X/2,C.ViewportSize.Y/2)local bt,bd=nil,CF.FV+1 local es=GE()for _,e in ipairs(es)do local tp=e.R if CF.AT=="HEAD"then tp=e.M:FindFirstChild("Head")or e.R elseif CF.AT=="TORSO"then tp=e.M:FindFirstChild("UpperTorso")or e.M:FindFirstChild("Torso")or e.R end
if tp then local sp,on=C:WorldToViewportPoint(tp.Position)if on then local d=(Vector2.new(sp.X,sp.Y)-sc).Magnitude if d<=CF.FV and d<bd then bd=d bt={P=tp,Pos=tp.Position,M=e.M}end end end end return bt end
local function AAT(t)if not t then return end local cp=C.CFrame.Position local dir=(t.Pos-cp).Unit local sm=CF.SM/100 local cl=C.CFrame.LookVector local nl=cl:Lerp(dir,sm)pcall(function()C.CFrame=CFrame.lookAt(cp,cp+nl)end)end
RS.RenderStepped:Connect(function()if CF.AA then local t=GBT()CT=t if t then AAT(t)else CT=nil end else CT=nil end end)
local function CE()for _,d in pairs(ED)do for _,v in pairs(d)do pcall(function()v:Remove()end)end end ED={}for _,d in pairs(HD)do pcall(function()d:Remove()end)end HD={}end
local function UE()if not CF.ESP or not DS then CE()return end local es=GE()local cm={}for _,e in ipairs(es)do cm[e.M]=true end for m,d in pairs(ED)do if not cm[m]then for _,v in pairs(d)do pcall(function()v:Remove()end)end ED[m]=nil end end
for _,e in ipairs(es)do local m=e.M local r=e.R local hd=e.H local he=m:FindFirstChild("Head")or r local rp,rv=C:WorldToViewportPoint(r.Position)local hp,hv=C:WorldToViewportPoint(he.Position)if rv or hv then
local sp=Vector2.new(rp.X,rp.Y)local bh=math.abs(rp.Y-hp.Y)*1.8 local bw=bh*0.6 if not ED[m]then local b=Drawing.new("Square")b.Visible=false b.Color=Color3.new(1,0,0)b.Thickness=1 b.Filled=false
local n=Drawing.new("Text")n.Visible=false n.Color=Color3.new(1,1,1)n.Size=16 n.Center=true n.Outline=true local hl=Drawing.new("Text")hl.Visible=false hl.Color=Color3.new(0,1,0)hl.Size=14 hl.Center=true hl.Outline=true
local dl=Drawing.new("Text")dl.Visible=false dl.Color=Color3.new(0.8,0.8,0.8)dl.Size=14 dl.Center=true dl.Outline=true ED[m]={B=b,N=n,H=hl,D=dl}end
local ed=ED[m]local b=ed.B local nl=ed.N local hl=ed.H local dl=ed.D b.Visible=true b.Position=Vector2.new(sp.X-bw/2,sp.Y-bh/2)b.Size=Vector2.new(bw,bh)nl.Visible=true nl.Text=m.Name nl.Position=Vector2.new(sp.X,sp.Y-bh/2-16)
local hea=hd.Health local mh=hd.MaxHealth local hp=hea/mh local hc=Color3.new(1-hp,hp,0)hl.Visible=true hl.Text=string.format("%.0f/%.0f",hea,mh)hl.Color=hc hl.Position=Vector2.new(sp.X,sp.Y-bh/2-2)
local dst=(C.CFrame.Position-r.Position).Magnitude dl.Visible=true dl.Text=string.format("%.0fm",dst)dl.Position=Vector2.new(sp.X,sp.Y+bh/2+2)else if ED[m]then for _,d in pairs(ED[m])do d.Visible=false end end end end end
local pls=0 local pdir=1 local function UH()if not CF.HB or not DS then for _,d in pairs(HD)do pcall(function()d:Remove()end)end HD={}return end pls=pls+pdir*0.05 if pls>1 then pdir=-1 pls=1 elseif pls<0 then pdir=1 pls=0 end
for m,c in pairs(HD)do if m~=(CT and CT.M)then pcall(function()c:Remove()end)HD[m]=nil end end if CT and CT.P then local m=CT.M local p=CT.P local pos,vis=C:WorldToViewportPoint(p.Position)if vis then
if not HD[m]then local c=Drawing.new("Circle")c.Visible=false c.Color=Cor.P c.Thickness=2 c.NumSides=20 c.Filled=false c.Radius=8 HD[m]=c end local c=HD[m]c.Visible=true c.Position=Vector2.new(pos.X,pos.Y)c.Color=Color3.new(0.3+pls*0.7,0.5+pls*0.5,1)c.Radius=8+pls*2 else if HD[m]then HD[m].Visible=false end end end end
RS.RenderStepped:Connect(function()UE()UH()end)
local function TG(p,y,t,v,d)CF[v]=d local f=Instance.new("Frame",p)f.Size=UDim2.new(1,-20,0,45)f.Position=UDim2.new(0,10,0,y)f.BackgroundTransparency=1
local l=Instance.new("TextLabel",f)l.Text=t l.Size=UDim2.new(0.7,0,1,0)l.Position=UDim2.new(0,0,0,0)l.BackgroundTransparency=1 l.TextColor3=Cor.T l.TextSize=16 l.Font=Enum.Font.Gotham l.TextXAlignment=Enum.TextXAlignment.Left
local b=Instance.new("TextButton",f)b.Size=UDim2.new(0,70,0,30)b.Position=UDim2.new(1,-70,0.5,-15)b.TextColor3=Cor.T b.TextSize=16 b.Font=Enum.Font.GothamBold b.BorderSizePixel=0 b.AutoButtonColor=false
Instance.new("UICorner",b).CornerRadius=UDim.new(0,8)local function up()b.BackgroundColor3=CF[v]and Cor.G or Cor.I b.Text=CF[v]and"ON"or"OFF"end up()b.MouseButton1Click:Connect(function()CF[v]=not CF[v]up()Notify(t.." "..(CF[v]and"ON"or"OFF"))if v=="BO"then FB(CF[v])end end)return 50 end
local function SL(p,y,t,v,mn,mx,df,sx)CF[v]=df local f=Instance.new("Frame",p)f.Size=UDim2.new(1,-20,0,65)f.Position=UDim2.new(0,10,0,y)f.BackgroundTransparency=1
local l=Instance.new("TextLabel",f)l.Text=t l.Size=UDim2.new(0.6,0,0,25)l.Position=UDim2.new(0,0,0,0)l.BackgroundTransparency=1 l.TextColor3=Cor.T l.TextSize=16 l.Font=Enum.Font.Gotham l.TextXAlignment=Enum.TextXAlignment.Left
local vl=Instance.new("TextLabel",f)vl.Size=UDim2.new(0.4,-10,0,25)vl.Position=UDim2.new(0.6,0,0,0)vl.BackgroundTransparency=1 vl.Text=tostring(df)..(sx or"")vl.TextColor3=Cor.P vl.TextSize=18 vl.Font=Enum.Font.GothamBold vl.TextXAlignment=Enum.TextXAlignment.Right
local bg=Instance.new("Frame",f)bg.Size=UDim2.new(1,0,0,20)bg.Position=UDim2.new(0,0,0,30)bg.BackgroundColor3=Cor.I bg.BorderSizePixel=0 Instance.new("UICorner",bg).CornerRadius=UDim.new(0,10)
local fl=Instance.new("Frame",bg)fl.Size=UDim2.new((df-mn)/(mx-mn),0,1,0)fl.BackgroundColor3=Cor.P fl.BorderSizePixel=0 Instance.new("UICorner",fl).CornerRadius=UDim.new(0,10)
local sb=Instance.new("TextButton",bg)sb.Size=UDim2.new(1,0,1,0)sb.BackgroundTransparency=1 sb.Text=""local drg=false local function upd(i)local pos=i.Position local ab=bg.AbsolutePosition local as=bg.AbsoluteSize.X local rel=math.clamp(pos.X-ab.X,0,as)local pct=rel/as local val=math.floor(mn+(mx-mn)*pct)CF[v]=val vl.Text=tostring(val)..(sx or"")fl.Size=UDim2.new(pct,0,1,0)end
sb.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then drg=true upd(i)end end)sb.InputChanged:Connect(function(i)if drg and(i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch)then upd(i)end end)sb.InputEnded:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then drg=false end end)return 70 end
